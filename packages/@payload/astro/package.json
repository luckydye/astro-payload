{
	"name": "@luckydye/astro-payload",
	"version": "0.0.12",
	"keywords": [
		"astro-integration"
	],
	"type": "module",
	"main": "./dist/adapter.js",
	"module": "./dist/adapter.js",
	"types": "./src/adapter.ts",
	"exports": {
		".": {
			"import": "./dist/adapter.js",
			"types": "./src/adapter.ts"
		},
		"./ts": {
			"import": "./src/adapter.ts",
			"types": "./src/adapter.ts"
		},
		"./server": {
			"import": "./dist/server.js",
			"types": "./src/server.ts"
		}
	},
	"scripts": {
		"build": "tsup"
	},
	"dependencies": {
		"fastify": "^4.15.0",
		"@fastify/express": "^2.3.0",
		"astro": "^2.2.0",
		"express": "^4.18.2",
		"payload": "^1.6.32"
	},
	"devDependencies": {
		"@types/express": "^4.17.17",
		"tsup": "^6.5.0"
	},
	"tsup": {
		"entry": [
			"src/server.ts",
			"src/adapter.ts"
		],
		"format": [
			"esm"
		],
		"dts": true
	},
	"files": [
		"README.md",
		"dist",
		"src",
		"adapter.ts",
		"server.ts",
		"types.ts"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/luckydye/astro-payload.git",
		"directory": "packages/@payload/astro"
	}
}
