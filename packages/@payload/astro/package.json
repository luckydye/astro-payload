{
	"name": "@luckydye/astro-payload",
	"version": "0.0.4",
	"keywords": [
		"astro-integration"
	],
	"type": "module",
	"main": "./dist/esm/adapter.js",
	"types": "./adapter.ts",
	"exports": {
		".": {
			"default": "./dist/esm/adapter.js",
			"import": "./dist/esm/adapter.js",
			"require": "./dist/cjs/adapter.js",
			"types": "./adapter.ts"
		},
		"./server": {
			"default": "./dist/esm/server.js",
			"import": "./dist/esm/server.js",
			"require": "./dist/cjs/server.js",
			"types": "./server.ts"
		}
	},
	"scripts": {
		"build": "tsc --outDir dist/esm && tsc -m commonjs --outDir dist/cjs"
	},
	"tsup": {
		"entry": [
			"./server.ts",
			"./adapter.ts"
		]
	},
	"dependencies": {
		"fastify": "^4.15.0",
		"@fastify/express": "^2.3.0",
		"astro": "^2.2.0",
		"express": "^4.18.2",
		"payload": "^1.6.32"
	},
	"devDependencies": {
		"@types/express": "^4.17.17"
	},
	"files": [
		"README.md",
		"dist",
		"adapter.ts",
		"server.ts",
		"types.ts"
	]
}
