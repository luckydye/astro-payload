{
	"name": "@payload/astro",
	"keywords": [
		"astro-integration"
	],
	"types": "module",
	"main": "./adapter.ts",
	"exports": {
		".": {
			"default": "./dist/adapter.mjs",
			"import": "./dist/adapter.mjs",
			"require": "./dist/adapter.js"
		},
		"./server": {
			"default": "./dist/server.mjs",
			"import": "./dist/server.mjs",
			"require": "./dist/server.js"
		}
	},
	"scripts": {
		"build": "tsup --format cjs,esm ./adapter.ts && tsup --format cjs,esm ./server.ts"
	},
	"dependencies": {
		"@fastify/express": "^2.3.0",
		"@fastify/static": "^6.10.0",
		"astro": "^2.2.0",
		"express": "^4.18.2",
		"fastify": "^4.15.0",
		"payload": "^1.6.32"
	},
	"devDependencies": {
		"@types/express": "^4.17.17",
		"tsup": "^6.5.0"
	}
}
