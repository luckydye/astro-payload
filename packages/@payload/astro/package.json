{
	"name": "@payload/astro",
	"keywords": [
		"astro-integration"
	],
	"type": "module",
	"main": "./adapter.ts",
	"types": "./dist/adapter.d.ts",
	"exports": {
		".": {
			"default": "./dist/adapter.js",
			"import": "./dist/adapter.js",
			"require": "./dist/adapter.cjs",
			"types": "./dist/adapter.d.ts"
		},
		"./server": {
			"default": "./dist/server.js",
			"import": "./dist/server.js",
			"require": "./dist/server.cjs",
			"types": "./dist/server.d.ts"
		}
	},
	"scripts": {
		"build": "tsup --dts --format cjs,esm ./adapter.ts && tsup --dts --format cjs,esm ./server.ts",
		"build:w": "tsup --watch --dts --format cjs,esm ./adapter.ts & tsup --watch --dts --format cjs,esm ./server.ts"
	},
	"dependencies": {
		"@fastify/express": "^2.3.0",
		"@fastify/static": "^6.10.0",
		"astro": "^2.2.0",
		"express": "^4.18.2",
		"fastify": "^4.15.0",
		"payload": "^1.6.32"
	},
	"devDependencies": {
		"@types/express": "^4.17.17",
		"tsup": "^6.5.0"
	}
}
