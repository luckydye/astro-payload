{
	"name": "web",
	"version": "1.0.0",
	"type": "module",
	"scripts": {
		"dev": "docker-compose up -d && dotenv -- astro dev --force",
		"build": "dotenv -- payload build && tsup && astro build",
		"serve": "dotenv -- node ./dist/server/entry.mjs"
	},
	"dependencies": {
		"@luckydye/astro-payload": "0.0.14",
		"astro": "^2.2.0",
		"payload": "^1.6.32"
	},
	"devDependencies": {
		"vite": "^4.2.1",
		"dotenv-cli": "^7.2.1",
		"tsup": "^6.5.0"
	},
	"tsup": {
		"entry": [
			"./payload.config.ts",
			"./payload.ts"
		],
		"format": [
			"cjs"
		]
	},
	"ts-node": {
		"transpileOnly": true
	}
}
