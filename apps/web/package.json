{
	"name": "web",
	"version": "1.0.0",
	"scripts": {
		"dev": "docker-compose up -d && dotenv -- astro dev",
		"types": "payload generate:types",
		"build": "dotenv -- payload build && astro build && tsup",
		"start": "PAYLOAD_CONFIG_PATH=./dist/payload.config.mjs dotenv -- node ./dist/server/entry.mjs"
	},
	"dependencies": {
		"@luckydye/astro-payload": "file:../../packages/@payload/astro",
		"@astrojs/node": "^5.1.1",
		"astro": "^2.2.0",
		"express": "^4.18.2",
		"payload": "^1.6.32"
	},
	"devDependencies": {
		"@types/express": "^4.17.17",
		"dotenv-cli": "^7.2.1",
		"tsup": "^6.5.0",
		"vite": "^4.2.1"
	},
	"tsup": {
		"entry": [
			"./payload.config.ts"
		],
		"format": [
			"esm"
		]
	}
}
