{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "moduleResolution": "node",
    "noEmit": true,
    // "allowImportingTsExtensions": true,
    "noImplicitAny": false,
    "useDefineForClassFields": false,
    "noImplicitReturns": false,
    "jsx": "preserve",
    "paths": {
      "~/*": ["./src/*"]
    }
  }
}
